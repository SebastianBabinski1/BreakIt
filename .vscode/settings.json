{
    "css.validate": true,
    "scss.validate": true,
    "stylelint.validate": [
        "scss"
    ],
    "stylelint.enable": true,
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true,
        "source.fixAll.stylelint": true
    },
    "eslint.alwaysShowStatus": true
}